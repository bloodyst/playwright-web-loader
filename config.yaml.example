limit_urls: 0
concurrency: 1

nav_timeout_ms: 45000
total_timeout_ms: 60000

max_retries: 1
retry_backoff_base_sec: 2.0
retry_jitter_sec: 0.7

min_delay_sec: 2.0
max_delay_sec: 4.0

headless: true

save_html_on_fail: true
save_screenshot_on_fail: true

out_dir: "out"

proxies:

  enabled: true
  random_proxy_per_task: false
  pages_per_proxy: 5
  list:
    - "example"
    - "example"
    - "example"
    - "example"
    - "example"
    - "example"
wait_until: domcontentloaded

post_nav_delay_sec: 0.4

challenge_wait_enabled: true
challenge_wait_sec: 8.0

locale: en-US
